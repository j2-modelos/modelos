    <modelo id="Prazo-Sem-Expediente-Forense"> <!-- ndlg7 -->
        <versao id="1.0">
            <classe id="Calendario" versao="empty">
                <constructs value="Certifico, ainda, que #:span@certItPrazoSemExpedienteForense-emCurso{no(s) dia(s) #:span@certItPrazoSemExpedienteForense-data{XX/XX/XXXX} 
                        #:U{não #:span@certItPrazoSemExpedienteForense-verHaver{houve/haverá} 
                        #:span@certItPrazoSemExpedienteForense-tipo{
                            #:span@certItPrazoSemExpedienteForense-tipo-subtipo{
                                expediente forense
                            }
                            #:span@certItPrazoSemExpedienteForense-tipo-abrangencia{
                                no TJMA
                            }}
                        } (#:span@certItPrazoSemExpedienteForense-data_{Dia do XXX}), 
                        #:span@certItPrazoSemExpedienteForense-transf{}} nos termos do(a) #:span@certItPrazoSemExpedienteForense-atoRes{Resolução GP 662021}." 
                    param="itemFormats" type="string"/>
                <constructs param="itemFormatsClass" value="p FntModDefault Indnt1cm marginAtoJudicial"  type="string" />
                <constructs param="mesesPraFrenteEPraTras" value="3"  type="string" />
                <constructs param="mostrarVazio" value="sim"  type="string" />
                
            </classe>
                        
            <!--elemento tag="p" scope="EDT" class="p FntDocEdt AlignCenter width100 lineHeightUnset marginTop8px">
                <elemento tag="span" scope="EDT">   
                    Datas suspensão expediente forense
                </elemento>
            </elemento-->
                        
            <classe id="Selector" versao="3.1" >
              <constructs param="newId" value="prazoSemExpedienteItens" type="string"/>
              <!--constructs param="linkedElTag" value="UL" type="string"/-->
              <constructs param="linkedElement" value="certItPrazoSemExpedienteForense-divContents" type="string"/>
              
              
              <constructs param="multiple" value="sim" type="bool"/>
              <!--constructs param="onChange" value="console.log('Missing onChange from certidaoItens (selector)');" type="string"/-->
              <constructs param="onAddAction" value="default" type="string"/>
              <constructs param="onDelAction" value="default" type="string"/>
              <constructs param="changeList" value="default" type="string"/>
              <!--constructs param="source" value="semExpedienteForenseSelSrc" type="bool"/-->
              <constructs param="source" value="#{j2.mod.clsCnstr.Calendario.selectorSource}" type="string"/>
              <constructs param="sourceLoadByExternalEvent" value="Calendario.afterParseSelectorXML" type="string"/>
              <!--constructs param="linkedElStyleClasses" value="p unorderedOrderedList FntModDefault paddingLeft35mm" type="string"/-->
              <!--constructs param="filter" value="true" type="boolean"/-->
            </classe> 
            
            
        </versao>
    
        <versao id="4.0">
            <classe id="Calendario" versao="empty">
                <constructs value="Certifico, 
                        #:span!j2-cert-sequencializador{ainda,}
                        que #:span@certItPrazoSemExpedienteForense-emCurso{no(s) dia(s) #:span@certItPrazoSemExpedienteForense-data{XX/XX/XXXX} 
                        #:U{
                            #:span@certItPrazoSemExpedienteForense-adverbial{não}
                            #:span@certItPrazoSemExpedienteForense-verHaver{houve/haverá} 
                        #:span@certItPrazoSemExpedienteForense-tipo{
                            #:span@certItPrazoSemExpedienteForense-tipo-subtipo{
                                expediente forense
                            }
                            #:span@certItPrazoSemExpedienteForense-tipo-abrangencia{
                                no TJMA
                            }}
                        } (#:span@certItPrazoSemExpedienteForense-data_{Dia do XXX}), 
                        #:span@certItPrazoSemExpedienteForense-transf{}} nos termos do(a) #:span@certItPrazoSemExpedienteForense-atoRes{Resolução GP 662021}
                        #:span@certItPrazoSemExpedienteForense-textoNegacaoDeMarcao{}
                        ." 
                    param="itemFormats" type="string"/>
                <constructs param="itemFormatsClass" value="p FntModDefault Indnt1cm marginAtoJudicial"  type="string" />
                <constructs param="mesesPraFrenteEPraTras" value="3"  type="string" />
                <constructs param="mostrarVazio" value="sim"  type="string" />
                
            </classe>
                        
            <!--elemento tag="p" scope="EDT" class="p FntDocEdt AlignCenter width100 lineHeightUnset marginTop8px">
                <elemento tag="span" scope="EDT">   
                    Datas suspensão expediente forense
                </elemento>
            </elemento-->
                        
            <classe id="Selector" versao="4.1" >
              <constructs param="newId" value="prazoSemExpedienteItens" type="string"/>
              <!--constructs param="linkedElTag" value="UL" type="string"/-->
              <constructs param="linkedElement" value="certItPrazoSemExpedienteForense-divContents" type="string"/>
              
              
              <constructs param="multiple" value="sim" type="bool"/>
              <!--constructs param="onChange" value="console.log('Missing onChange from certidaoItens (selector)');" type="string"/-->
              <constructs param="onAddAction" value="default" type="string"/>
              <constructs param="onDelAction" value="default" type="string"/>
              <constructs param="changeList" value="default" type="string"/>
              <!--constructs param="source" value="semExpedienteForenseSelSrc" type="bool"/-->
              <constructs param="source" value="#{j2.mod.clsCnstr.Calendario.selectorSource}" type="string"/>
              <constructs param="sourceLoadByExternalEvent" value="Calendario.afterParseSelectorXML" type="string"/>
              <!--constructs param="linkedElStyleClasses" value="p unorderedOrderedList FntModDefault paddingLeft35mm" type="string"/-->
              <!--constructs param="filter" value="true" type="boolean"/-->
              <!--constructs param="label" value="Atos" type="string"/-->
              <constructs param="hideLabel" value="true" type="boolean"/>

              <!-- versão 4.1 -->
                <constructs param="mostrarToggler" value="nao" type="boolean"/>

                <constructs param="botaoAuxiliar" value="sim" type="boolean"/>
                <constructs param="botaoAuxiliarFaIcont" value="fa-calendar-alt" type="string"/>
                <constructs param="botaoAuxiliarOnClick" value="#{j2.mod.clsCnstr.Calendario.abrirCalendarioJ2}" type="string"/>

            </classe> 
            
            
        </versao>
